#! /bin/sh

destination=$workingcopy"/Packages/complete/ccprobe-windows"
download=$workingcopy"/Packages/download/ccprobe-windows"
bin=$workingcopy"/CCProbe/bin"
components=$workingcopy"/Packages/components/ccprobe-windows"
doc=$workingcopy"/Packages/doc"
labbook=$workingcopy"/TeemssXML/LabBookXSL"

rm -f $destination/*
rm -f $download/*

cp $bin/CCProbe.jar  $destination
cp $doc/ccprobe-windows/* $destination
cp $doc/all/* $destination
cp $labbook/LabBook.PDB $destination
cp $components/* $destination

echo $destination
ls -l $destination

osascript make-windows.applescript

echo $download
ls -l $download






