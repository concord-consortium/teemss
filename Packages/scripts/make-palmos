#! /bin/sh

destination="/Users/stephen/cvsroot/releases/complete/ccprobe-palmos"
download="/Users/stephen/cvsroot/releases/download/ccprobe-palmos"
bin="/Users/stephen/cvsroot/CCProbe/bin"
components="/Users/stephen/cvsroot/releases/components/ccprobe-palmos"
doc="/Users/stephen/cvsroot/releases/doc"
labbook="/Users/stephen/cvsroot/Xml/LabBookXSL"

rm -f $destination/*
rm -f $download/*

cp $bin/CCProbe.prc  $destination
cp $doc/ccprobe-palmos/* $destination
cp $doc/all/* $destination
cp $labbook/LabBook.PDB $destination

# cp $components/* $destination

echo $destination
ls -l $destination

osascript make-palmos.applescript

cd $destination/../
tar -czvf $download/CCProbe.tgz ccprobe-palmos

rm -f $download/CCProbe.sea

echo $download
ls -l $download



