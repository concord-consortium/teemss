#! /bin/sh

destination="/Users/stephen/cvsroot/releases/complete/ccprobe-macosx"
download="/Users/stephen/cvsroot/releases/download/ccprobe-macosx"
bin="/Users/stephen/cvsroot/CCProbe/bin"
components="/Users/stephen/cvsroot/releases/components/ccprobe-macosx"
doc="/Users/stephen/cvsroot/releases/doc"
labbook="/Users/stephen/cvsroot/Xml/LabBookXSL"

rm -rf $destination/*
rm -rf $download/*

cp $doc/ccprobe-macosx/* $destination
cp $doc/all/* $destination
cp $labbook/LabBook.PDB $destination
/Developer/Tools/CpMac -r $components/* $destination
cp $bin/CCProbe.jar  $destination/CCProbe.app/Contents/Resources/Java/            

echo $destination
ls -l $destination

osascript make-macosx.applescript

rm -f $download/CCProbe.sea

echo $download
ls -l $download







