#! /bin/sh

destination=$workingcopy"/Packages/complete/ccprobe-macosx"
download=$workingcopy"/Packages/download/ccprobe-macosx"
bin=$workingcopy"/CCProbe/bin"
components=$workingcopy"/Packages/components/ccprobe-macosx"
doc=$workingcopy"/Packages/doc"
labbook=$workingcopy"/TeemssXML/LabBookXSL"

rm -rf $destination/*
rm -rf $download/*

cp $doc/ccprobe-macosx/* $destination
cp $doc/all/* $destination
cp $labbook/LabBook.PDB $destination
/Developer/Tools/CpMac -r $components/* $destination
cp $bin/CCProbe.jar  $destination/CCProbe.app/Contents/Resources/Java/            

echo $destination
ls -l $destination

osascript make-macosx.applescript

rm -f $download/CCProbe.sea

echo $download
ls -l $download







