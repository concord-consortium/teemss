#! /bin/sh

destination=$workingcopy"/Packages/complete/ccprobe-pocketpc"
download=$workingcopy"/Packages/download/ccprobe-pocketpc"
bin=$workingcopy"/CCProbe/bin"
components=$workingcopy"/Packages/components/ccprobe-pocketpc"
doc=$workingcopy"/Packages/doc"
labbook=$workingcopy"/TeemssXML/LabBookXSL"

rm -rf $destination/*
rm -rf $download/*

cp $bin/CCProbe.wrp  $destination
cp $bin/CCProbe.lnk  $destination
cp $doc/ccprobe-pocketpc/* $destination
cp $doc/all/* $destination
cp $labbook/LabBook $destination
cp -r $components/* $destination

echo $destination
ls -l $destination

osascript make-pocketpc.applescript

rm $download/CCProbe.sit

echo $download
ls -l $download







