#! /bin/sh

destination="/Users/stephen/cvsroot/Packages/complete/ccprobe-pocketpc"
download="/Users/stephen/cvsroot/Packages/download/ccprobe-pocketpc"
bin="/Users/stephen/cvsroot/CCProbe/bin"
components="/Users/stephen/cvsroot/Packages/components/ccprobe-pocketpc"
doc="/Users/stephen/cvsroot/Packages/doc"
labbook="/Users/stephen/cvsroot/TeemssXML/LabBookXSL"

rm -rf $destination/*
rm -rf $download/*

cp $bin/CCProbe.wrp  $destination
cp $bin/CCProbe.lnk  $destination
cp $doc/ccprobe-pocketpc/* $destination
cp $doc/all/* $destination
cp $labbook/LabBook.PDB $destination
cp -r $components/* $destination

echo $destination
ls -l $destination

osascript make-pocketpc.applescript

rm $download/CCProbe.sit

echo $download
ls -l $download







