         <LINE xaxis="0" yaxis="0" datasource="dc-f3"/>
	</GRAPH>
      </DATACOLLECTOR>


      <DATACOLLECTOR ID="dc-force4" name="Measure Force" >
       <PROBE ID="dc-f4" probe="Force">
         <PROP name="Mode" value="End of Arm"/>
         <PROP name="Range" value="+/- 20N"/>
         <PROP name="Speed" value="400 per second"/>
       </PROBE>
       <GRAPH current-line="0" title="">
         <XAXIS min="0" max="10"/>
         <YAXIS min="-20" max="1"/>
         <LINE xaxis="0" yaxis="0" datasource="dc-f4"/>
	</GRAPH>
      </DATACOLLECTOR>

      <DATACOLLECTOR ID="dc-smartwheel-pos1" name="Measure Position" >
        <PROBE ID="dc-sw-pos1" probe="SmartWheel">
         <PROP name="Port" value="A"/>
       </PROBE>
        <GRAPH current-line="0" title="">
          <XAXIS min="0" max="40"/>
          <YAXIS min="-0.5" max="6"/>
          <LINE xaxis="0" yaxis="0">
            <INTPROBETRANS quantity="Position" probe="dc-sw-pos1"/></LINE>
	</GRAPH>
      </DATACOLLECTOR>

      <DATACOLLECTOR ID="dc-smartwheel-pos2" name="Measure Position" >
        <PROBE ID="dc-sw-pos2" probe="SmartWheel">
         <PROP name="Port" value="A"/>
       </PROBE>
        <GRAPH current-line="0" title="">
